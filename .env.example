
# Supabase Configuration
VITE_SUPABASE_URL=https://your-project-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
VITE_SUPABASE_FUNCTIONS_URL=https://your-project-url.supabase.co/functions/v1

# Cloudinary Configuration
VITE_CLOUDINARY_CLOUD_NAME=your-cloud-name
VITE_CLOUDINARY_API_KEY=your-api-key
VITE_ENABLE_CLOUDINARY=false

# Feature Flags
VITE_ENABLE_WASM_PROCESSING=true
VITE_ENABLE_ANALYTICS=false

# Analytics Configuration (client-side)
VITE_POSTHOG_API_KEY=your-posthog-key
VITE_POSTHOG_HOST=https://app.posthog.com
VITE_SENTRY_DSN=your-sentry-dsn

# Server-side only variables (not prefixed with VITE_)
# These should only be configured on the server, not in .env files
# OPENAI_API_KEY=your-openai-api-key
# CLOUDINARY_API_KEY=your-api-key
# CLOUDINARY_API_SECRET=your-api-secret
# STRIPE_SECRET_KEY=your-stripe-key
# STRIPE_WEBHOOK_SECRET=your-webhook-secret
